[req]
default_bits       = 2048
prompt             = no
default_md         = sha256
req_extensions     = req_ext
distinguished_name = dn

[dn]
C                  = US
ST                 = California
L                  = San Francisco
O                  = MyCompany
OU                 = MyDepartment
CN                 = localhost

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1   = localhost
IP.1    = 127.0.0.1
IP.2    = 10.0.8.106
